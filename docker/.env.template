# PostgreSQL Configuration
PG_PASS=your_secure_database_password_here
PG_USER=authentik
PG_DB=authentik

# Authentik Configuration
AUTHENTIK_SECRET_KEY=your_authentik_secret_key_here
AUTHENTIK_BOOTSTRAP_PASSWORD=your_initial_admin_password_here
# AUTHENTIK_IMAGE=ghcr.io/goauthentik/server
# AUTHENTIK_TAG=2025.10.3

# Compose port mappings (optional)
# COMPOSE_PORT_HTTP=9000
# COMPOSE_PORT_HTTPS=9443

# Cloudflare Tunnel Token
# Get this from: https://one.dash.cloudflare.com/
# Create a tunnel and configure it to point to http://server:9000
CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token_here

# Data path (base directory for bind mounts)
DATA_PATH=/data/lab-authentik-development